  <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="/assets/css/reset.css">
  <link rel="stylesheet" href="/assets/css/global.css">
  <link rel="stylesheet" href="/assets/css/components/header.css">
  <link rel="stylesheet" href="/assets/css/components/footer.css">
  <script src="https://kit.fontawesome.com/9dfe5f8b66.js" crossorigin="anonymous"></script>
  <style>
    main {
        min-height: 100vh;
    }
  </style>
  <title>{% block titleHead %}{% endblock %}</title>

  {% block head %}{% endblock %}
</head>
<body>
  {{ include('/layouts/components/header/index.twig') }}
  <main>
    {% block body %}
    {% endblock %}
  </main>

  {{ include('/layouts/components/footer/index.twig') }}
  {% block scriptsBody %}{% endblock %}
  <script src="/assets/js/components/mobile-navbar.js"></script>
  <script>
    const mobileNavbar = new MobileNavbar('#bars', '.header__nav__list', '.header__nav__list__itens');
  </script>
</body>
</html>